<div class="page-header">
  <%= link_to requisitions_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_requisition_path(@requisition), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show requisition</h1>
</div>

<dl class="dl-horizontal">
  <dt>Id:</dt>
  <dd><%= @requisition.id %></dd>

  <dt>Title:</dt>
  <dd><%= @requisition.title %></dd>

  <dt>Description:</dt>
  <dd><%= @requisition.description %></dd>

  <dt>Amount:</dt>
  <dd><%= @requisition.amount %></dd>

  <dt>Status:</dt>
  <dd><%= @requisition.status %></dd>

  <dt>Location:</dt>
  <dd><%= @requisition.loc %></dd>

</dl>
